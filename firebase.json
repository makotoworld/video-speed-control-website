{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "images/.*",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.@(mp4|webm|ogg)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "video/mp4"
          },
          {
            "key": "Accept-Ranges",
            "value": "bytes"
          }
        ]
      }
    ]
  }
}
